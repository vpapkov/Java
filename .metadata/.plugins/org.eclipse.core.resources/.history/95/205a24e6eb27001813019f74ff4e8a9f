public class Cars extends Vehicle
{
	private int aWheels;
	private String model;
	private String maker;
	private String year;	
	
	public Cars (String aType, int aaaWheels,  String amodel , String amaker, String ayear)
	{
	super(aType, aaaWheels);		
	model = amodel;
	maker = amaker;
	year = ayear;
	}
	
	public String getModel()
	{
		return model;
	}
	public String getMaker()
	{
		return maker;
	}
	public String getyear()
	{
		return year;
	}

}