public class Vehicle

{
private String type;
private String Wheels;
//private String model;
//private String year;

public Vehicle (String atype, String theWheels)

{
  type = atype;
  Wheels = theWheels;
}

public String getType()
	{
		return type;
	}

public String getwheels()
	{
		return Wheels;
	}
//public String getamodel()
//{
//	return model;
//}
//public String getyear()
//	{
//		return year;
//	}

}
