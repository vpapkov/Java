public class Vehicle {
	
private String VehType;
private int VehWheels;


public Vehicle (String ValueKotoroePhiydetType, int ValueKotoroePhiydetWheels) {
  VehType = ValueKotoroePhiydetType;
  VehWheels = ValueKotoroePhiydetWheels;
}

public String getType() {
		return VehType;
}

public int getwheels() {
		return VehWheels;
}

}