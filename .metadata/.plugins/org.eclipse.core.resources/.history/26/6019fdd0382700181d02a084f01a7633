
public class Point

{
	
private double x;
private double y;

public double distanseFromOregin()
	{
	return Math.sqrt(x*x + y * y); 	
	}

public double getX()
{
	return x;
}

public void setX(double x)
{
	this.x = x;
}

public double getY()
{
	return y;
}

public void setY(double y)
{
	this.y = y;
}


// constructor

public Point(double x, double y)
	{
	this.x = x;
	this.y = y;
	}

}