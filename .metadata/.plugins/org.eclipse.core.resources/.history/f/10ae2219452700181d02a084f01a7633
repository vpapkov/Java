public class Cars extends VehicleNew
{
	private String model;
	private String maker;
	private String Year;	
	
	public Cars (String amodel, String amaker, String aYear)
	{
	super(amodel);	
	model = amodel;
	maker = amaker;
	Year = aYear;
	}
	
	public void Drive()
	{
		System.out.println("This car can drive");
	}
}