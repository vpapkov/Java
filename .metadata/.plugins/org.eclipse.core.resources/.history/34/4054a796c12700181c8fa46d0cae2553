public class Cars extends Vehicle
{
	private static int aWheels;
	private String model;
	private String maker;
	private String year;	
//	private int theWheels;
	
	public Cars (String aType, /**int atheWheels,*/  String amodel , String amaker, String ayear)
	{
	super(aType  /**,atheWheels*/, aWheels);		
	model = amodel;
	maker = amaker;
	year = ayear;
//	theWheels = atheWheels;
	}
	
	public String getModel()
	{
		return model;
	}
	public String getMaker()
	{
		return maker;
	}
	public String getyear()
	{
		return year;
	}
//	public int theWheels() {
//		return theWheels;
//	}
}