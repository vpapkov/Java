public class Vehicle

{
private String type;
private int Wheels;
private String model;
private String year;

public Vehicle (String atype, int theWheels, String amodel, String year)

{
  type = atype;
  Wheels = theWheels;
}

public String getType()
	{
		return type;
	}

public int getwheels()
{
	return Wheels;
}
public String getamodel()
{
	return model;
	
public String getyear()
	{
		return year;
	}
}
}
